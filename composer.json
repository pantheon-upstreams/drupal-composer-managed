{
    "name": "pantheon-upstreams/drupal-composer-managed",
    "description": "Install Drupal 9 with Composer on Pantheon.",
    "type": "project",
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "path",
            "url": "upstream-configuration"
        },
        {
            "type": "vcs",
            "url": "https://git.drupalcode.org/sandbox/asherry-3335446.git"
        },
        {
            "type": "vcs",
            "url": "https://bitbucket.org/atendesigngroup/mercury_editor.git"
        }
    ],
    "require": {
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "drupal/address": "~1.0",
        "drupal/admin_toolbar": "~3.0",
        "drupal/allowed_formats": "^2.0",
        "drupal/apitools": "dev-1.x",
        "drupal/auto_entitylabel": "^3.0",
        "drupal/backup_migrate": "~5.0",
        "drupal/better_exposed_filters": "^6.0",
        "drupal/cl_generator": "^1.0@RC",
        "drupal/cl_server": "^2.0",
        "drupal/components": "^3.0",
        "drupal/core": "^10",
        "drupal/core-composer-scaffold": "^10",
        "drupal/core-recommended": "^10",
        "drupal/ctools": "~3.0",
        "drupal/date_range_formatter": "^4.0",
        "drupal/download_request": "1.0.x-dev",
        "drupal/entity": "^1.4",
        "drupal/entity_reference_revisions": "~1.0",
        "drupal/entityqueue": "~1.0",
        "drupal/field_group": "~3.0",
        "drupal/field_tools": "~1.0",
        "drupal/filefield_paths": "~1.0",
        "drupal/gin": "^3.0@RC",
        "drupal/gin_toolbar": "^1.0@RC",
        "drupal/google_analytics": "^4.0",
        "drupal/honeypot": "~2.0",
        "drupal/image_widget_crop": "^2.4",
        "drupal/imce": "^3.0",
        "drupal/inline_entity_form": "~1.0",
        "drupal/layout_paragraphs": "^2.0",
        "drupal/mailsystem": "^4.4",
        "drupal/maxlength": "^2.0",
        "drupal/menu_block": "~1.0",
        "drupal/menu_item_extras": "^2.19",
        "drupal/mercury_editor": "2.1.x-dev@dev",
        "drupal/metatag": "~1.0",
        "drupal/migrate_drupal_d8": "^1.0",
        "drupal/name": "^1.0",
        "drupal/notification_message": "1.x-dev@dev",
        "drupal/optional_end_date": "^1.3",
        "drupal/page_manager": "^4.0@RC",
        "drupal/paragraphs": "~1.0",
        "drupal/pathauto": "~1.0",
        "drupal/rabbit_hole": "^2.0",
        "drupal/redirect": "~1.0",
        "drupal/scheduler": "~1.0",
        "drupal/search_api": "^1.28",
        "drupal/search_api_block": "^1.1",
        "drupal/search_api_pantheon": "^8.1",
        "drupal/search_api_solr": "^4.2",
        "drupal/similarterms": "^1.8",
        "drupal/simple_block": "~1.0",
        "drupal/smart_date": "^3.7",
        "drupal/state_machine": "^1.6",
        "drupal/structure_sync": "^2.0",
        "drupal/style_options": "1.0.x-dev@dev",
        "drupal/token": "~1.0",
        "drupal/twig_field_value": "~2.0",
        "drupal/twig_tweak": "^3.2",
        "drupal/views_filters_summary": "^2.0@alpha",
        "drupal/views_infinite_scroll": "^2.0",
        "drupal/viewsreference": "^2.0@beta",
        "drupal/webform": "^6.2",
        "drupal/webform_content_creator": "^4.0",
        "drupal/webform_user_registration": "^1.0@alpha",
        "drush/drush": "^11 || ^12",
        "mglaman/composer-drupal-lenient": "^1.0",
        "pantheon-systems/drupal-integrations": "^10",
        "pantheon-upstreams/upstream-configuration": "dev-main",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "drupal/core-dev": "^10",
        "drupal/devel": "^5.1",
        "drupal/devel_entity_updates": "^4.1",
        "drupal/migrate_plus": "^6.0",
        "drupal/migrate_tools": "^6.0",
        "drupal/stage_file_proxy": "^2.0",
        "drupal/upgrade_status": "^4.0",
        "weitzman/drupal-test-traits": "^2.0"
    },
    "conflict": {
            "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "./web"
            },
            "allowed-packages": [
                "pantheon-systems/drupal-integrations"
            ],
            "file-mapping": {
                "[project-root]/.editorconfig": false,
                "[project-root]/pantheon.upstream.yml": false,
                "[project-root]/.gitattributes": false
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "web/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/profiles/custom/{$name}": ["type:drupal-custom-profile"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"],
            "web/private/scripts/quicksilver/{$name}/": ["type:quicksilver-script"]
        },
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
          "drupal/apitools": {
            "Workflow state action fixes": "https://git.drupalcode.org/project/apitools/-/merge_requests/3.diff"
          },
          "drupal/core": {
            "Issue #3129179: Provide some way to rebuild the persistent bundle field map": "https://www.drupal.org/files/issues/2023-08-30/rebuild_field_map-3129179-39.patch"
          },
          "drupal/ctools": {
            "Cannot use Entity View block in Layout Builder when there is no context": "https://www.drupal.org/files/issues/2021-05-24/ctools-layout_builder_context-3029323-12.patch"
          },
          "drupal/entity": {
            "Add an overridden add page with destination support": "https://git.drupalcode.org/project/entity/-/merge_requests/8.diff"
          },
          "drupal/notification_message": {
            "Block filtering by current timezone and not UTC": "https://www.drupal.org/files/issues/2023-02-17/3342687_0.patch",
            "Issue #3388715: Error when deleting message type": "https://git.drupalcode.org/project/notification_message/-/merge_requests/17.diff"
          },
          "drupal/page_manager": {
            "\"Assigned contexts were not satisfied: user\" due to PageAccessCheck->access": "https://www.drupal.org/files/issues/2022-11-24/3323572-catch-context-exeptions-2.diff"
          },
          "drupal/webform_user_registration": {
            "Issue #3304147: Automated Drupal 10 compatibility fixes": "https://www.drupal.org/files/issues/2022-12-24/webform_user_registration.1.x-dev.rector.patch"
          },
          "drupal/views_filters_summary": {
            "Issue #3290489: Automated Drupal 10 compatibility fixes": "https://git.drupalcode.org/project/views_filters_summary/-/merge_requests/15.diff"
          }
        },
        "drupal-lenient": {
            "allowed-list": [
                "drupal/views_filters_summary",
                "drupal/webform_user_registration"
            ]
        }
    },
    "autoload": {
        "classmap": [
            "upstream-configuration/scripts/ComposerScripts.php"
        ]
    },
    "scripts": {
        "pre-update-cmd": [
            "DrupalComposerManaged\\ComposerScripts::preUpdate"
        ],
        "upstream-require": [
            "DrupalComposerManaged\\ComposerScripts::upstreamRequire"
        ]
    },
    "scripts-descriptions": {
        "upstream-require": "Add a dependency to an upstream. See https://pantheon.io/docs/create-custom-upstream for information on creating custom upstreams."
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "8.1"
        },
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/console-extend-plugin": true,
            "mglaman/composer-drupal-lenient": true,
            "phpstan/extension-installer": true
        }
    }
}
