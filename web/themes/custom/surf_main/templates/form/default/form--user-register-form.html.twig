{#
/**
 * @file
 * Theme override for a 'form' element.
 *
 * Available variables:
 * - attributes: A list of HTML attributes for the wrapper element.
 * - children: The child elements of the form.
 *
 * @see template_preprocess_form()
 */
#}

{% embed 'surf_main:form' with {
    attributes,
    title_prefix,
    title_suffix,
    children,
    form_attributes: create_attribute(),
    form_type: 'page',
} only %}
    {% block form_items %}
        {{ title_prefix }}
        {{ title_suffix }}
        {{ children }}
    {% endblock %}
{% endembed %}

