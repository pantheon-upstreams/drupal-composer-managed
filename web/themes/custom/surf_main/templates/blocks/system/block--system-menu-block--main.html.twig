{#
/**
 * @file
 * Theme override for the a Primary Menu navigation block.
 *
 * Available variables:
 * - No additional variables are available for this block type.
 *
 * @see templates/blocks/system/block--system-menu-block.html.twig
 * @see components/02-molecules/menu/menu.twig
 */
#}

{% embed 'surf_main:menu' with {
    title_prefix,
    title_suffix,
    menu_attributes: create_attribute({'class': 'm-menu--primary'}).merge(attributes),
    menu_title: 'Primary menu'|t,
    menu_content: content,
} only %}
    {% block menu_prefix %}
        {{ title_prefix }}
        {{ title_suffix }}
    {% endblock %}
{% endembed %}
