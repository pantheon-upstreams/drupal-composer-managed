<article{{attributes.addClass(['article'])}}>
	{# article__header l-constrained #}
	<header class="">
		{{ title_prefix }}
		{% include 'page-title' with {
    text: label
    } only %}
		{{ title_suffix }}
	</header>
	{# l--1-col l-constrained #}
	<div class="">
		<div class="l__primary">
			{% if content %}
				{{ content }}
			{% endif %}
			<h2>Component Library</h2>
			{# Add Components here üëáüèª #}

			<!-- Page Heading / Hero -->

			<!-- Accordion -->
			<h3>Accordion</h3>
			<p>Coming soon...</p>

			<!-- Pullquote -->
			<div class="l-stack">
				<h3>Pullquote Fullwidth</h3>
				{% include "pullquote" with {
					quote: '<p>Extremely large emphasized text example. Lorem ipsum dolor sit amet.</p>',
					credit: 'Optional second line for quote credit.',
					style:'fullwidth'
				} %}
				<div class="t-pane t-forest u-padding">
					{% include "pullquote" with {
						quote: '<p>Extremely large emphasized text example. Lorem ipsum dolor sit amet.</p>',
						credit: 'Optional second line for quote credit.',
						style:'fullwidth'
					} %}
				</div>
				<h3>Pullquote Centered</h3>
				{% include "pullquote" with {
					quote: '<p>‚ÄúLearn from yesterday, live for today, hope for tomorrow. The important thing is not to stop questioning.‚Äù</p>',
					credit: 'Albert Enstein',
					style:'centered'
				} %}
				<div class="t-pane t-forest u-padding">
					{% include "pullquote" with {
						quote: '<p>‚ÄúLearn from yesterday, live for today, hope for tomorrow. The important thing is not to stop questioning.‚Äù</p>',
						credit: 'Albert Enstein',
						style:'centered'
					} %}
				</div
			</div>


			<!-- Callouts & Factoids -->
				<div class="l-stack"> <h3>Callouts</h3>
				{% include "callout" with {
					heading: 'It\'s a Callout!',
					body: 'This is a callout component. It\'s a great way to highlight important information.',
					link_text: 'Click Me',
					link_url: 'http://example.com',
					background_style: 'color',
					color: 'cloud',
				} %}
				{% include "callout" with {
					heading: 'It\'s a Callout!',
					body: 'This is a callout component. It\'s a great way to highlight important information.',
					link_text: 'Click Me',
					link_url: 'http://example.com',
					background_style: 'color',
					color: 'forest'
				} %}
				{% embed "callout" with {
					heading: 'It\'s a Callout!',
					body: 'This is a callout component. It\'s a great way to highlight important information.',
					link_text: 'Click Me',
					link_url: 'http://example.com',
					background_style: 'color',
					color: 'kelly'
				} %}
					{% block callout__link %}
						<div class="c-callout__link">
							{% include "link-button" with {
							text: link_text,
							href: link_url,
							color: 'white',
							iconType: 'arrow',
						} %}
						</div>
					{% endblock %}
				{% endembed %}
			</div>

			{# End of Components üëÜüèª #}
			<hr/>

			{# Start of HTML Kitchen Sink #}

			<div>
				<h3>
					Examples of Buttons & Links
				</h3>
				{% include "link-button" with {
          text: "Forest Green Button Component",
          url: "google.com",
					color: 'forest',
          uppercase: true,
        } %}
				<p>
					<a href="#" class="forest-button">Forest Green Button Class</a>
				</p>
				{% include "link-button" with {
					text: "Lowercase Button with Arrow Component",
					url: "apple.com",
					color: 'forest',
					uppercase: false,
					icon_type: 'arrow'
				} %}
				<p>Button wth arrow example</p>
